<div class="fea_block container-fluid" style="background-color:skyblue">
  {{#each notes}}
  <div class="s-item-container" style="display: block;">
    <div class="a-fixed-left-grid" style="padding-left:218px">
      <div class="col-sm-8" style="border-style: solid; background-color: lightblue; border-width: 1px; margin: 10px; border-radius: 10px; border-color: #32CD32;">
        <div class="a-fixed-left-grid-col a-col-left" style="width:200px; padding: 3px; float:left">
          <img style="width: 100%" src="{{this.pdf}}" />
        </div>
        <div class="a-fixed-left-grid-col a-col-right" style="float: right; padding: 1px; width: 70%;">
          <div style="float: left; display: block; width: 100%;">
            <a href="/product/{{this.id}}" title="{{this.name}}"><h2>{{this.name}} <a style="font-size: 60%">in {{this.subject}}</a></h2></a>
            <div style="width: 50%; float: left;">
              <p>by {{this.owner.username}}</p>
              <p>at in {{this.school.name}}</p>
                <hr />
              </div>
              <div style="width: 50%; float: right;">
                <p>${{this.price}}.00</p>
                <p>{{this.rating}}/5</p>
                <hr />
              </div>
            </div>

            <p style="display:block">{{this.description}}</p>

          </div>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
  {{else}}
  <!--Features block-->
